		{
			"name": "GET List of books",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200 for GET list of books\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"\r",
							"pm.test(\"GET List of books has 6 objects\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.length).to.eql(6);\r",
							"});\r",
							"pm.test(\"GET List of books contains specific data for list object\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData[2].id).to.eql(3);\r",
							"    pm.expect(jsonData[2].name).to.eql(\"The Vanishing Half\");\r",
							"    pm.expect(jsonData[2].type).to.eql(\"fiction\");\r",
							"    pm.expect(jsonData[2].        available).to.eql(true);\r",
							"    console.log(jsonData[2])\r",
							"});\r",
							"\r",
							"pm.test(\"GET list of books contains <The Russian>\", function () {\r",
							"    pm.expect(pm.response.text()).to.include(\"The Russian\");\r",
							"});\r",
							"\r",
							"\r",
							"pm.test(\"Response time is less than 700ms\", function () {\r",
							"    pm.expect(pm.response.responseTime).to.be.below(700);\r",
							"});\r",
							"const nonFiction = pm.response.json().filter((book) => book.available === true);  \r",
							"console.log(nonFiction)\r",
							"\r",
							"const cartifiction = pm.response.json().filter((book) => book.type === \"fiction\");  \r",
							"console.log(cartifiction)\r",
							"\r",
							"\r",
							"pm.test(\"Obține toate cărțile fără parametri\", function () {\r",
							"    pm.expect(pm.response.json()).to.be.an('array');\r",
							"});"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
